#Examples
In js 
1.  let my_name = prompt("What is your name")
    console.log(my_name);

2.  let y = 0
    while(true) {
        if(y == 1){
            break
        }
        y += 1
    }

3. let our_array = [1,2,3]
   func sum_array_elements(our_array) {
    let sum = 0
    for(let i = 0; i < our_array.length; i++) {
        sum += our_array[i]
    }
    return sum
   }

4. class Dog {
    constructor() {
        this.age = 0
    }
    birthday() {
        this.age += 1
    }
   }
  Dog d = new Dog()
  d.birhtday()
  console.log(d.age)

5. const language_name = "River"
   language_name = "Pyth" //Would throw an error
    

In River
1. text my_name = inflow("What is your name")
   outflow(my_name)

2.  num y ~ 0
    loop()
        if(y == 1)
            dry
        y +~ 1
3.  Stream our-array ~ [1,2,3]
    Task sum_array_elements(our-array)
        num sum ~ 0
        loop(num n in our-array)
            sum +~ n
        return sum
4. Model Dog
    num age = 0
    Task birthday()
        age +~ 1
  Dog d = new Dog()
  d.birthday()
  outflow(d.age)
    
5. frozen Text language_name = "River"
   language_name = "Pyth"   // Would throw an error
        




